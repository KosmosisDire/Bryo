type Vehicle {
    var speed = 0;
    var fuel = 100;
    var engineRunning = false;
    
    fn startEngine() {
        engineRunning = true;
        fuel = fuel - 5;
    }
    
    fn accelerate(i32 amount): i32 {
        if (engineRunning) {
            speed = speed + amount;
            fuel = fuel - amount;
            return speed;
        }
        return 0;
    }
    
    fn getFuelLevel(): i32 {
        return fuel;
    }
    
    fn isMoving(): bool {
        return speed > 0;
    }
}

type BankAccount {
    var balance = 1000;
    var isActive = true;
    
    fn deposit(i32 amount): i32 {
        balance = balance + amount;
        return balance;
    }
    
    fn withdraw(i32 amount): bool {
        if (balance >= amount && isActive) {
            balance = balance - amount;
            return true;
        }
        return false;
    }
    
    fn getBalance(): i32 {
        return balance;
    }
}

fn main(): i32 {
    var car = new Vehicle();
    car.startEngine();
    
    var newSpeed = car.accelerate(50);
    var currentFuel = car.getFuelLevel();
    var moving = car.isMoving();
    
    var account = new BankAccount();
    var depositResult = account.deposit(500);
    var withdrawSuccess = account.withdraw(200);
    var finalBalance = account.getBalance();
    
    if (moving && withdrawSuccess) {
        return newSpeed + finalBalance;
    }
    
    return 0;
}