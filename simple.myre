

type String
{
    i8[12] data;
}

fn PrintString(String s)
{
    Print(s.data);
}

fn StrLen(i8[] s)
{
    f32 length = 0.0;
    i32 i = 0;
    while (s[i] != '\0')
    {
        length += 1.0;
        i++;
    }
    return length;
}

fn Main()
{
    String s;
    s.data = ['H', 'e', 'l', 'l', 'o', ' ', 'W', 'o', 'r', 'l', 'd', '\0'];

    PrintString(s);

    return StrLen(s.data);
}