type Test
{
    bool stuff = true
}

fn RunTest(bool* test)
{
    Print("Running test...\n")
    if (test[0])
    {
        Print("Test stuff is true\n")
        test[0] = false
    }
    else
    {
        Print("Test stuff is false\n")
    }
}

fn RunTests(Test* test)
{
    test[0].stuff = true
    RunTest(&test[0].stuff)
    RunTest(&test[0].stuff)
}

fn Main
{
    Test test = new Test(true)
    RunTests(&test)
    return 0.0
}