type Test
{
    bool stuff = true

    fn Run()
    {
        if (stuff)
        {
            return 1.0
        }

        return 0.0
    }

    fn Run2()
    {
        Run()
        return Run() + 10.0
    }

    fn Run3(i32 depth)
    {
        if (depth > 10)
        {
            return 15.0
        }

        return Run3(depth + 1)
    }
}

fn Main
{
    Test test = new Test(true)
    f32 stuff = 1.0
    stuff += 10.0
    
    return stuff
} 