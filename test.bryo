type Animal
{
    var health = 10
    var IsDead => health <= 0

    new(var startingHealth)
    {
        health = startingHealth
    }

    fn Speak
    {
        return 1.0
    }

    fn Heal(var amount)
    {
        health += amount
        return health
    }

}

fn Test(Animal a)
{
    return a.Speak()
}

fn Main
{
    if 10 > 5 && 5 < 10
    {
        return 0.0
    }

    if 10 < 5
    {
        return 5.0
    }

    for (var i = 0; i < 10; i = i + 1)
    {
        var x = i * 2
    }

    var a = new Animal()
    a.Speak()
    a.health = 5

    var big = new Animal(50)
    big.Heal(10)
    big.Speak()
    big.health -= a.health

    return Test(big)
} 
